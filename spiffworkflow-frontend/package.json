  "overrides": {
    "postcss-preset-env": {
      "autoprefixer": "10.4.5"
    }
  },
  "scripts": {
    "build": "vite build",
    "sync-bpmn-package": "rsync -av --delete packages/bpmn-js-spiffworkflow-react/ ../../bpmn-js-spiffworkflow-react/ --exclude node_modules --exclude dist --exclude .git --exclude .gitignore --exclude .npmignore",
    "eslint": "./node_modules/.bin/eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write --list-different src/**/*.[tj]s{,x}",
    "lint": "./node_modules/.bin/eslint src --ext .js,.jsx,.ts,.tsx && npm run typecheck",
    "lint:fix": "./node_modules/.bin/eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "serve": "vite preview",
    "start": "VITE_VERSION_INFO='{\"version\":\"local\"}' vite",
    "startopen": "VITE_VERSION_INFO='{\"version\":\"local\"}' vite --host 0.0.0.0",
    "test": "vitest run --coverage",
    "typecheck": "./node_modules/.bin/tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/carbon__colors": "^10.31.3",
